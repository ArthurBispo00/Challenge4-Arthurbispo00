{
  "expo": {
    "name": "RadarMotu",
    "slug": "radarmotu",
    "scheme": "radarmotu",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "51.0.0",
    "plugins": [

      "expo-dev-client", 

      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "minSdkVersion": 23
          },
          "ios": {}
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#22DD44",
          "sounds": []
        }
      ]
      
    ],
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "com.arthurbispo00.radarmotuapp", 
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.VIBRATE",
        "android.permission.INTERNET"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.arthurbispo00.radarmotuapp", 
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "O Radar usa Bluetooth para localizar sua TAG pr√≥xima."
      }
    },
    "extra": {
      "eas": {
        "projectId": "efa16ba2-6119-4693-b839-14a9e56d6ce7"
      }
    },
    "owner": "arthurbispo" 
  }
}